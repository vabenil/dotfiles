nnoremap <C-w> :quit<CR>
nnoremap <C-l> :sh! xdotool key --window $VIMB_XID alt+n<CR><Esc>
nnoremap <C-h> :sh! xdotool key --window $VIMB_XID alt+p<CR><Esc>
nnoremap <A-l> :sh! xdotool key --window $VIMB_XID alt+n<CR><Esc>
nnoremap <A-h> :sh! xdotool key --window $VIMB_XID alt+p<CR><Esc>
nnoremap gt :sh! xdotool key --window $VIMB_XID alt+n<CR><Esc>
nnoremap gT :sh! xdotool key --window $VIMB_XID alt+p<CR><Esc>
nnoremap 1gt :sh! xdotool key --window $VIMB_XID ctrl+1<CR><Esc>
nnoremap 2gt :sh! xdotool key --window $VIMB_XID ctrl+2<CR><Esc>
nnoremap 3gt :sh! xdotool key --window $VIMB_XID ctrl+2<CR><Esc>
nnoremap 3gT :sh! xdotool key --window $VIMB_XID ctrl+3<CR><Esc>
nnoremap 4gT :sh! xdotool key --window $VIMB_XID ctrl+4<CR><Esc>
nnoremap 5gt :sh! xdotool key --window $VIMB_XID ctrl+5<CR><Esc>
nnoremap 6gt :sh! xdotool key --window $VIMB_XID ctrl+6<CR><Esc>
nnoremap 7gT :sh! xdotool key --window $VIMB_XID ctrl+7<CR><Esc>
nnoremap 8gT :sh! xdotool key --window $VIMB_XID ctrl+8<CR><Esc>
nnoremap 9gt :sh! xdotool key --window $VIMB_XID ctrl+9<CR><Esc>
nnoremap 10gT :sh! xdotool key --window $VIMB_XID ctrl+10<CR><Esc>
nnoremap M :sh! mpv <C-R>%<CR>
nnoremap ss :set stylesheet!|no! R<CR>
nnoremap ZZ :q<CR>
nnoremap i <C-Z>

set stylesheet=false

set home-page=https://duckduckgo.com
set download-path=/home/vabenil/Downloads

set history-max-items=200
set input-autohide=true
set x-hint-command=:sh! mpv <C-R>;<CR>

shortcut-default dd
shortcut-add dd=https://duckduckgo.com/?q=$0
shortcut-add yt=https://www.youtube.com/results?search_query=$0
shortcut-add wiki=https://en.wikipedia.org/w/index.php?search=$0



